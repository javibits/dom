<odoo>
    <record id="patient_diagnosis_tree_view" model="ir.ui.view">
      <field name="name">dom.patient.diagnosis.tree</field>
      <field name="model">dom.patient.diagnosis</field>
      <field name="arch" type="xml">
        <tree sample="1" editable="top">
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record id="patient_diagnosis_action" model="ir.actions.act_window">
      <field name="name">Diagnosis</field>
      <field name="res_model">dom.patient.diagnosis</field>
      <field name="view_mode">tree</field>
      <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face">
          Add a new Diagnosis
        </p>
      </field>
    </record>

</odoo>